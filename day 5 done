// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int t;
    printf("Enter the bettery low");
    scanf("%d",&t);
    while(t>0)
    {
        int x;
        scanf("%d",&x);
        if (x<=15)
        printf("yes\n");
        else 
        printf("No\n");
        
    }

    return 0;
}
